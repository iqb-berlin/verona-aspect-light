    "@iqb/eslint-config": "1.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "iqb-dev-components": "^1.3.11",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
